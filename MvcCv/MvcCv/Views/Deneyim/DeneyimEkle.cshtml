@model MvcCv.Models.Entity.TblDeneyimlerim
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />

<form method="post" class="form-">
    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı Giriniz" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlık" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Tarihi Giriniz" />
    <br />
    <textarea placeholder="Açıklamanızı Giriniz" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

